<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Limitless_SaleProject2</display-name>
  <servlet>
    <servlet-name>ServletMarketPlace</servlet-name>
    <servlet-class>servlets.ServletMarketPlace</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletIdentity_register</servlet-name>
    <servlet-class>servlets.ServletIdentity_register</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletIdentity_login</servlet-name>
    <servlet-class>servlets.ServletIdentity_login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletIdentity_logout</servlet-name>
    <servlet-class>servlets.ServletIdentity_logout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletIdentity_validate</servlet-name>
    <servlet-class>servlets.ServletIdentity_validate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletChat_service</servlet-name>
    <servlet-class>servlets.ServletChat_service</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletMarketPlace</servlet-name>
    <url-pattern>/views/marketplace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletIdentity_register</servlet-name>
    <url-pattern>/views/identity_register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletIdentity_login</servlet-name>
    <url-pattern>/identity_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletIdentity_logout</servlet-name>
    <url-pattern>/views/identity_logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletIdentity_validate</servlet-name>
    <url-pattern>/views/validate_access</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletChat_service</servlet-name>
    <url-pattern>/views/chat_service</url-pattern>
  </servlet-mapping>
</web-app>